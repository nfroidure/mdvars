{
  "name": "mdvars",
  "version": "1.0.0",
  "description": "Extract vars from markdown files and output the markdown.",
  "homepage": "https://github.com/nfroidure/mdvars",
  "main": "src/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha tests/*.mocha.js",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- tests/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha --report html -- tests/*.mocha.js -R spec -t 5000"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nfroidure/mdvars"
  },
  "keywords": [
    "markdown",
    "var",
    "stream",
    "varstream",
    "metadata"
  ],
  "dependencies": {
    "varstream": "0.3.x"
  },
  "devDependencies": {
    "coveralls": "~2.11.2",
    "istanbul": "~0.3.17",
    "mocha": "~2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "streamtest": "^1.2.1"
  },
  "author": {
    "name": "Nicolas Froidure",
    "url": "http://www.insertafter.com/blog.html"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/nfroidure/mdvars/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/nfroidure/mldvars/issues"
  }
}
